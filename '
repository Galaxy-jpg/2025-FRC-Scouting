function submit() {
  gatherValues();

  listedValues = "";
  for (let i = 0; i < values.length; i++) {
    listedValues += values[i] + ";";
  }
  genQR(listedValues);
  console.log(listedValues);
}

function increase(section, name) {
  document.getElementById(section).elements.name.value.stepUp();
}
